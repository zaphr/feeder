<!DOCTYPE html>
<html>
  <head>
    <title>Feeder</title>
      <%= stylesheet_link_tag "/jqtouch/jqtouch.min.css" %>
      <%= stylesheet_link_tag "/jqtouch/themes/apple/theme.min.css" %>
      <%= javascript_include_tag "/jqtouch/jquery.1.3.2.min.js" %>
      <%= javascript_include_tag "/jqtouch/jqtouch.min.js" %>
      <%= javascript_include_tag "mobile" %>
  </head>
  <body>
    <div class="current">
      <div class="toolbar">
        <h1><%= @title || "meFind.me" %></h1>
		<% unless @home_page %>
          <a class='back rounded flip'>Back</a>
		  <a href="/" class='button rightButton flip'>New Search</a>
		<% end %>
      </div>  
      <%= yield %>
    </div>
  </body>
</html>
